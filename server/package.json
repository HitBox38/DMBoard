{
  "name": "dmboard-server",
  "version": "1.0.0",
  "description": "Download JSON files from a specific folder in a public GitHub repository",
  "main": "index.ts",
  "scripts": {
    "build": "tsc",
    "start": "nodemon src/index.ts",
    "lint": "eslint src/**/*.{js,ts,json}",
    "lint:fix": "eslint --fix src/**/*.{js,ts,json}",
    "format": "prettier --write src/**/*.{js,ts,md,json} --config ./.prettierrc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/yourrepo.git"
  },
  "author": "yourname",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.26.1",
    "body-parser": "^1.20.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.8",
    "@types/nodemon": "^1.19.2",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  }
}
